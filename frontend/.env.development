# Development Environment Configuration
NODE_ENV=development

# API Configuration
VITE_API_BASE_URL=http://localhost:8015
VITE_WS_URL=ws://localhost:8000/ws

# Development Authentication Bypass
VITE_AZURE_CLIENT_ID=development-client-id
VITE_AZURE_TENANT_ID=development-tenant-id
VITE_AZURE_REDIRECT_URI=http://localhost:3000

# Feature Flags for Development
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_PWA=false
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_DEBUG=true
VITE_ENABLE_PERFORMANCE_MONITORING=false

# Development Bypass for Authentication
VITE_DEV_BYPASS_AUTH=true
VITE_DEV_USER_EMAIL=developer@aeolitech.com
VITE_DEV_USER_NAME=Development User
VITE_DEV_USER_ROLE=admin

# Logging
VITE_LOG_LEVEL=debug
VITE_ENABLE_ERROR_REPORTING=false