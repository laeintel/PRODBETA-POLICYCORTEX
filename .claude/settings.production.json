{
  "permissions": {
    "allow": [
      "Bash(cargo build:*)",
      "Bash(cargo test:*)",
      "Bash(cargo run)",
      "Bash(npm install)",
      "Bash(npm run build)",
      "Bash(npm run test)",
      "Bash(docker build:*)",
      "Bash(docker-compose up:*)",
      "Bash(terraform plan)",
      "Bash(terraform apply)",
      "Bash(git status)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push)",
      "Bash(az account show)",
      "Bash(./scripts/test-production-local.bat)",
      "Bash(./scripts/start-local.bat)"
